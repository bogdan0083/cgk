//- title: Index Page
extends layouts/_layout
block head
  - var title = 'Popups'
  - var n = 0
  - var i = 0
  - var servicesTextArr = ['Технический <br/> план', 'Межевой<br/>план', 'Акт<br/> обследования', 'Выписки<br/> из ЕГРН', 'Геодезическая<br/> съемка и обмеры', 'Схема расположения<br/>земельного участка', 'Исправление<br/> реестровых ошибок', 'Проектная<br/> документация', ' Кадастровый учет<br/> и регистрация прав', 'Юридические<br/> услуги']
  - var worktypesTextArr = ['Земельные <br/> участки', 'Здания <br/> и сооружения', 'Нежилые <br/> помещения', 'Квартиры <br/> и комнаты',  'Юридические <br/> услуги']
block header
  - var headerInner = true
  - var active = 1
  - var submenu = true
  - var submenuActive = 0
block content
  // start layout
  .layout.layout-services
    .container
      h1.layout-heading Наши услуги и цены
      .services-block-group
        a.service-block-item(href="#")
          .wrapper
            .service-img(style="background: url(../images/service-thumb1.jpg) no-repeat center / cover")
            .content
              h4.service-item-heading Технический план
              p.text Документ, необходимый для государственного кадастрового учета объектов капитального строительства
          .bottom
            span Подробнее
        a.service-block-item(href="#")
          .wrapper
            .service-img(style="background: url(../images/service-thumb2.jpg) no-repeat center / cover")
            .content
              h4.service-item-heading Межевой план
              p.text Документ, необходимый для кадастрового учета земельного участка или части земельного участка
          .bottom
            span Подробнее
        a.service-block-item(href="#")
          .wrapper
            .service-img(style="background: url(../images/service-thumb2.jpg) no-repeat center / cover")
            .content
              h4.service-item-heading Акт обследования
              p.text Документ, необходимый для снятия объекта капитального строительства с кадастрового учета
          .bottom
            span Подробнее
        a.service-block-item(href="#")
          .wrapper
            .service-img(style="background: url(../images/service-thumb2.jpg) no-repeat center / cover")
            .content
              h4.service-item-heading Выписки из ЕГРН
              p.text Сведения ЕГРН об объекте, зарегистрированных правах на него, копии документов, содержащихся в реестре
          .bottom
            span Подробнее
        a.service-block-item(href="#")
          .wrapper
            .service-img(style="background: url(../images/service-thumb2.jpg) no-repeat center / cover")
            .content
              h4.service-item-heading Юридические услуги
              p.text Узаконивание перепланировок, подготовка документов, полное сопровождение сделки, консультации
          .bottom
            span Подробнее
        a.service-block-item(href="#")
          .wrapper
            .service-img(style="background: url(../images/service-thumb2.jpg) no-repeat center / cover")
            .content
              h4.service-item-heading Геодезическая съемка и обмеры
              p.text Комплекс работ, в процессе которого выполняется получение данных для составления чертежей, планов, схем
          .bottom
            span Подробнее
        a.service-block-item(href="#")
          .wrapper
            .service-img(style="background: url(../images/service-thumb2.jpg) no-repeat center / cover")
            .content
              h4.service-item-heading Схема расположения земельного участка
              p.text Документ, необходимый для формирования нового земельного участка на кадастровом плане территории
          .bottom
            span Подробнее
        a.service-block-item(href="#")
          .wrapper
            .service-img(style="background: url(../images/service-thumb2.jpg) no-repeat center / cover")
            .content
              h4.service-item-heading Исправление реестровых ошибок
              p.text Работы, направленные для устранения ошибок в сведениях ЕГРН и документах, на основании которых сведения были внесены в базу
          .bottom
            span Подробнее
        a.service-block-item(href="#")
          .wrapper
            .service-img(style="background: url(../images/service-thumb2.jpg) no-repeat center / cover")
            .content
              h4.service-item-heading Проектная документация
              p.text Документы, необходимые для узаконивания перепланировок либо переустройств объектов капитального строительства
          .bottom
            span Подробнее
        a.service-block-item(href="#")
          .wrapper
            .service-img(style="background: url(../images/service-thumb2.jpg) no-repeat center / cover")
            .content
              h4.service-item-heading Кадастровый учет и регистрация прав
              p.text Комплекс мероприятий для постановки объектов на учет, внесение изменений в сведения ЕГРН, регистрации прав и обременений 
          .bottom
            span Подробнее
  // end layout
  // start questions
  .questions
    .container
      h2.section-heading Если у Вас возникли вопросы,
